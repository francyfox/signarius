{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@app/*": [
        "src/*"
      ],
      "@schema/*": [
        "console/module/formkit/export/*"
      ]
    }
    //    "types": ["bun-types"]
  }
}